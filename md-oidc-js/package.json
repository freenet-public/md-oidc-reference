{
  "name": "md-oidc-js",
  "version": "1.0.0",
  "description": "Mobilcom-Debitel OIDC JavaScript library",
  "main": "lib/md-oidc.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require babel-register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-register --colors -w ./test/*.spec.js"
  },
  "keywords": [
    "webpack",
    "es6",
    "starter",
    "library",
    "universal",
    "umd",
    "commonjs"
  ],
  "author": "Jan Singer",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "*",
    "@babel/core": "*",
    "@babel/preset-env": "*",
    "babel-eslint": "*",
    "babel-loader": "*",
    "babel-plugin-add-module-exports": "*",
    "babel-preset-env": "*",
    "babel-register": "*",
    "chai": "*",
    "eslint": "*",
    "eslint-loader": "*",
    "jsdom": "*",
    "jsdom-global": "*",
    "mocha": "*",
    "uglifyjs-webpack-plugin": "*",
    "webpack": "*",
    "webpack-cli": "*",
    "yargs": "*"
  },
  "dependencies": {
    "base64-js": "^1.3.0",
    "base64js": "^1.0.1",
    "jquery": "^3.5.0",
    "js-sha256": "^0.9.0",
    "lodash": ">=4.17.11",
    "loglevel": "^1.6.1"
  }
}
